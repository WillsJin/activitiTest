  <process id="testBusinessTripFlow2" name="出差申请流程2">
    <startEvent id="startevent1" name="启动出差申请" activiti:formKey="bussinessTripForm"></startEvent>
    <serviceTask id="mailtask1" name="发送邮件" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to" expression="zhichao.zheng@travelzen.com"></activiti:field>
        <activiti:field name="from" expression="zhichao.zheng@travelzen.com"></activiti:field>
        <activiti:field name="subject" expression="您提交了一份出差申请"></activiti:field>
        <activiti:field name="charset" expression="UTF-8"></activiti:field>
        <activiti:field name="html">
          <activiti:expression><![CDATA[HI]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <userTask id="usertask2" name="组长审批" activiti:assignee="gonzo" activiti:formKey="bussinessTripAdvice"></userTask>
    <exclusiveGateway id="exclusivegateway1" name="判断金额"></exclusiveGateway>
    <userTask id="usertask3" name="主管审批" activiti:assignee="fozzie"></userTask>
    <userTask id="usertask4" name="财务审批" activiti:assignee="kermit"></userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <serviceTask id="mailtask2" name="邮件通知" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to" expression="zhichao.zheng@travelzen.com"></activiti:field>
        <activiti:field name="from" expression="zhichao.zheng@travelzen.com"></activiti:field>
        <activiti:field name="subject" expression="审批结束通知"></activiti:field>
        <activiti:field name="charset" expression="UTF-8"></activiti:field>
        <activiti:field name="html">
          <activiti:expression><![CDATA[审批结束哦！${fee},${advice}]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow3" name="" sourceRef="mailtask1" targetRef="usertask2"></sequenceFlow>
    <sequenceFlow id="flow5" name="&gt;=5000" sourceRef="exclusivegateway1" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${fee>=5000}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" name="&lt;5000" sourceRef="exclusivegateway1" targetRef="usertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${fee<5000}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow7" name="" sourceRef="usertask2" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow8" name="" sourceRef="usertask3" targetRef="usertask4"></sequenceFlow>
    <sequenceFlow id="flow9" name="" sourceRef="usertask4" targetRef="mailtask2"></sequenceFlow>
    <sequenceFlow id="flow10" name="" sourceRef="mailtask2" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow11" name="" sourceRef="startevent1" targetRef="mailtask1"></sequenceFlow>
  </process>
